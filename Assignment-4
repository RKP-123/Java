package pkg1;

public class This_Keyword_Use {
	
	public This_Keyword_Use()
	{
		this(7,8,9);
		System.out.println("Default Constructor");
	}
	public This_Keyword_Use(int a)
	{
		this();
		System.out.println("1 Parameter Constructor");
	}
	public This_Keyword_Use(int a, int b)
	{
		this(5);
		System.out.println("2 Parameter Constructor");
	}
	public This_Keyword_Use(int a, int b, int c)
	{
		System.out.println("3 Parameter Constructor");
	}
	
	public static void main(String[] args) {
		
		This_Keyword_Use ref=new This_Keyword_Use(10, 20);
	}

}
